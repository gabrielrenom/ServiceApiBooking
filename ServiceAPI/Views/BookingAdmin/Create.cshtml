                                    </label>
                                    @Html.EditorFor(model => model.Car.Registration, new { htmlAttributes = new { @class = "form-control", placeholder = "ie: A3456DF", type = "text" } })
                                    @Html.ValidationMessageFor(model => model.Car.Registration, "", new { @class = "text-danger" })
